    "@types/sinon": "17.0.3",
    "@types/sinon-chai": "^4.0.0",
    "chai": "^4.3.7",
    "chokidar": "^4.0.3",
    "del-cli": "^6.0.0",
    "eslint": "^8.56.0",
    "mocha": "^11.1.0",
    "nyc": "^17.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^19.0.2",
