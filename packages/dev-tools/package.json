{
  "name": "@sitecore-content-sdk/dev-tools",
  "version": "0.1.0-beta.5",
  "description": "Utilities to assist in the development of Sitecore JSS apps.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-esm.json",
    "watch": "npm run build -- --watch",
    "clean": "del-cli dist types",
    "lint": "eslint \"./src/**/*.ts\"",
    "test": "mocha --require ts-node/register -r tsconfig-paths/register  \"./src/**/*.test.ts\"",
    "prepublishOnly": "npm run build",
    "coverage": "nyc npm test",
    "generate-docs": "npx typedoc --plugin typedoc-plugin-markdown --outputFileStrategy Members --parametersFormat table --readme none --out ../../ref-docs/dev-tools src/index.ts --githubPages false"
  },
  "engines": {
    "node": ">=22"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://jss.sitecore.com",
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@sitecore-content-sdk/core": "0.1.0-beta.5",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "del": "^8.0.0",
    "glob": "^11.0.1",
    "recast": "^0.23.9",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/register": "^7.25.9",
    "@types/chai": "^4.3.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.9.0",
    "@types/sinon": "^17.0.3",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "del-cli": "^6.0.0",
    "eslint": "^8.56.0",
    "mocha": "^11.1.0",
    "nyc": "^17.1.0",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "~5.7.3"
  },
  "types": "types/index.d.ts",
  "gitHead": "2f4820efddf4454eeee58ed1b2cc251969efdf5b",
  "files": [
    "dist",
    "types",
    "/*.js",
    "/*.d.ts"
  ]
}
